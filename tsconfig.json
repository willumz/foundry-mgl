{
  "compilerOptions": {
    "target": "es2016",
    "module": "commonjs",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./scripts",
    "rootDir": "./src/scripts",
    "allowJs": true,
  },
    "include": ["src/scripts/**/*"],
}
